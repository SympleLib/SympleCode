#define VECTOR__METADATA_SIZE 8

ptr Vector__New()
{
	ptr rvec = calloc(VECTOR__METADATA_SIZE);
	return rvec + VECTOR__METADATA_SIZE;
};

int Vector__GetSize(ptr vector)
{
	ptr rvec = vector - VECTOR__METADATA_SIZE;
	return *rvec;
};

void _Vector__SetSize(ptr vector, int size)
{
	ptr sz = @size;
	ptr rvec = vector - VECTOR__METADATA_SIZE;
	rvec[0] = sz[0];
	rvec[1] = sz[1];
	rvec[2] = sz[2];
	rvec[3] = sz[3];
};