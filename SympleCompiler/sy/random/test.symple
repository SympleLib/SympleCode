extern int printf(string fmt);
extern int puts(string str);

int main()
{
	int myInt = 6 * 9;
	byte myByte = 4 * 20;
	
	printf("%i\n", -myInt + myByte);
	
	myInt = myByte + 4;
	
	printf("%i\n", myInt);

	printf("Number: %i, Even: %s\n", myInt, BoolToString(!(myInt % 2)));
	
	0;
};

string BoolToString(bool boolean)
	if (boolean)
		return "true";
	else
		return "false";