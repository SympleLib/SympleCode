extern int printf(string fmt);
extern int puts(string str);
extern int putc(char c, ptr stream);
extern ptr __acrt_iob_func(int num);

#define stdin  __acrt_iob_func(0)
#define stdout __acrt_iob_func(1)
#define stderr __acrt_iob_func(2)

int main()
{
	int myInt = 6 * 9;
	byte myByte = 4 * 20;
	
	printf("%i\n", -myInt + myByte);
	
	myInt = myByte + 3;
	
	printf("%i\n", myInt);

	printf("Number: %i, Even: %s\n", myInt, BoolToString(!(myInt % 2)));
	char sad = 'E';
	putc(sad, stdout);
		
	0;
};

string BoolToString(bool boolean)
	if (boolean)
		"true";
	else
		"false";