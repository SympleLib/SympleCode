#include "Node.sy"

#include "libc/memory.sy"

ptr Node__New(ptr data, ptr previous, ptr next)
{
	Node this = malloc(NODE_SIZE);
	
	memcpy(this + 0, @data, 4);
	memcpy(this + 4, @previous, 4);
	memcpy(this + 8, @next, 4);
};

ptr Node__Previous(ptr this)
{
	return this + 4;
};

ptr Node__Next(ptr this)
{
	return this + 8;
};

ptr Node__Delete(ptr this)
{
	free(this);
};