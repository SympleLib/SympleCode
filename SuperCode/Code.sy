decl uint time(byte*);
decl void seed srand(uint);
decl uint rand();

decl void print _putws(char*);
decl void printf wprintf(char*, ...);

void flip(uint flip-count)
{
	seed(time(null));
	uint n = rand();
	uint x = 2;
	uint py = 0;

	while flip-count > 0
	{
		uint y = n % x;
		printf('%i\n', y - py != 0);
		flip-count = flip-count - 1;
		x = x * 2;
		py = y;
	}
}

void run() ->
	flip(8);