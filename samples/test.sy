// link 'link.sy';

@naked decl int MessageBoxA(byte*, char*, char*, int);

class MessageBox {
	char* title, message,
	int type;
	
	int show() {
		ret MessageBoxA(null, message, title, type);
	}
	
	void show(int nTimes) {
		for (int i = 0; i < nTimes; i++)
			MessageBoxA(null, message, title, type);
	}
}

void run() {
	// loadDoTheThing('this is the string to print out');
	// loadDoTheThing();
	// doTheThing();
	
	MessageBox msg = MessageBox { 'class test', 'it works?', 1 };
	msg.show(5);
	msg.message = 'hell ya it does!';
	msg.show();
}