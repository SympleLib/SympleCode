decl void printf(char* fmt, ...)
decl void puts(char*)

void run {
	call(&test);
}

void test() {
	Test x;
	printTest(&x);
}

void call(void()* func) {
	%func();
}

void printTest(Test* ptr) {
	printf('x = %i, y = %i, z = %i, w = %i\n', %ptr.x, %ptr.y, %ptr.z, %ptr.w);
	puts(%ptr.a.test);
	puts('Hello, world!');
}

struct Test {
	int x = 69,
	int y = 420,
	int z = 1337,
	int w = 0,
	X a,
}

struct X {
	char* test = 'haha this works, I am so proud!',
}