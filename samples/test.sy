int powi(int base, int exp) {
    int result = 1;
    while (exp) {
        if (exp % 2)
           result *= base;
        exp /= 2;
        base *= base;
    }
    ret result;
}

void run {
	int a = 6;
	int b = 6;

	printf('+%i', a + b);
	printf('-%i', a - b);
	printf('*%i', a * b);
	printf('/%i', a / b);
	printf('%%i', a % b);
	printf('**%i', a ** b);
	printf('&%i', a & b);
	printf('|%i', a | b);
	printf('^%i', a ^ b);
	printf('&&%i', a && b);
	printf('||%i', a || b);
	printf('<%i', a < b);
	printf('>%i', a > b);
	printf('>=%i', a <= b);
	printf('<=%i', a >= b);
	printf('~=%i', a ~= b);
	printf('==%i', a == b);
	printf('!=%i', a != b);
}