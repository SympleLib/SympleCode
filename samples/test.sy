decl void printf(char* fmt, ...)
decl void putchar(char)
decl void puts(char*)

void run {
	float* arr = float [ 6.9, 4.20, 1.337 ];
	
	printInt((int) arr[0]); // explicit cast
	printInt([int] arr[1]); // bit cast
	printInt(arr[2]);        // implicit cast
	
	putchar(`\n`);
	
	printInt(arr[0] to int); // explicit cast
	printInt(arr[1] as int); // bit cast
	printInt(arr[2]);        // implicit cast
}

void printInt(int n) {
	printf('%i\n', n);
}