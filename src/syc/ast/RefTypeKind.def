#ifndef REFTYPE
#define REFTYPE(x, y)
#endif

REFTYPE(Value, "$") // copy
REFTYPE(Weak, "*") // readonly ref (default)
REFTYPE(Owned, "^") // pass ownership
REFTYPE(Temp, "&") // ref
REFTYPE(Shared, "%") // share ownership

#undef REFTYPE